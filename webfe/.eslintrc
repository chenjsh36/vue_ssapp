{
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
		"$": true,
		"jQuery": true,
		"FBDate":true,
		"PIXI":true,
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"destructuring": true,
		"classes": true,
		"defaultParams": true,
		"blockBindings": true,
		"modules": true,
		"objectLiteralComputedProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"restParams": true,
		"spread": true,
		"templateStrings": true
	},

	"rules": {
		"no-undef": 1,
		"no-empty": 1,
		"no-dupe-args": 1,
		"no-cond-assign": 1,
		"no-dupe-keys": 1,
		"no-unused-vars": 1,
		"no-empty-character-class": 1,
		"no-func-assign": 1,
		"no-redeclare": 1,
		"no-unreachable": 1,
		"eqeqeq": 1,
		"brace-style": 1
	}
}